{
    "custom/wireguard": {
        "interval": 3,
        "return-type": "json",
        "format-icons": {
            "connected": "<span color=\"#50fa7b\">  </span>",
            "disconnected": "<span color=\"#ff5555\">  </span>"
        },
        "on-click": "$HOME/.config/hypr/scripts/wireguard.sh -t",
        "exec": "$HOME/.config/hypr/scripts/wireguard.sh -s",
        "format": "{icon}"
    }
}